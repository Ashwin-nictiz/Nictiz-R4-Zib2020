<ConceptMap xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir">
    <id value="NummerSoortCodelijst-to-ContactPointUse"/>
    <extension url="http://hl7.org/fhir/StructureDefinition/concept-bidirectional">
        <valueBoolean value="true"/>
    </extension>
    <url value="http://nictiz.nl/fhir/ConceptMap/NummerSoortCodelijst-to-ContactPointUse"/>
    <version value="1.0.2"/>
    <name value="NummerSoortCodelijst-to-ContactPointUse"/>
    <status value="active"/>
    <publisher value="Nictiz"/>
    <contact>
      <name value="Nictiz"/>
      <telecom>
        <system value="email"/>
