        <source value="http://terminology.hl7.org/CodeSystem/v3-AddressUse"/>
        <target value="http://hl7.org/fhir/contact-point-use"/>
        <element>
            <code value="HP"/>
            <target>
                <code value="home"/>
                <equivalence value="equal"/>
                <comment value="HP: Primary Home/Telefoonnummer thuis."/>
            </target>
        </element>
        <element>
            <code value="TMP"/>
            <target>
                <code value="temp"/>
                <equivalence value="equal"/>
                <comment value="TMP: Temporary Address/Tijdelijk telefoonnummer."/>
            </target>
        </element>
        <element>
            <code value="WP"/>
            <target>
                <code value="work"/>
                <equivalence value="equal"/>
                <comment value="WP: Work Place/Zakelijk telefoonnummer."/>
            </target>
        </element>
    </group>
</ConceptMap>